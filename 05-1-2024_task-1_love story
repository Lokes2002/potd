import java.util.*;
 class lovestory{
     public static void main(String [] args) {
         int count=0;
         String s ="codeforces";
         Scanner sc = new Scanner(System.in);
         int n = sc.nextInt();
         String ip = sc.nextLine();
         for(int i=0; i<ip.length(); i++) {
             char ch = ip.charAt(i);
             char sh = s.charAt(i);
             if(ch!=sh) {
                 count++;
             }
         }
         System.out.println(count);
     }
 }
